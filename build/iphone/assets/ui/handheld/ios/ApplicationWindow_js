function ApplicationWindow(e,t){var i=(require("services/rss"),require("ui/common/MasterView")),o=require("ui/common/DetailView");require("ui/common/Description");var a=Ti.UI.createWindow({backgroundColor:"#ffffff",navBarHidden:!0}),n=Ti.UI.createView({backgroundColor:"#000",top:0,height:20});a.add(n);var r=new i(e),d=new o,l=Ti.UI.createWindow({title:t,navBarHidden:!1,barImage:"navbar.png",titleControl:Ti.UI.createLabel({text:t,color:"white",font:{fontFamily:"HelveticaNeue-CondensedBold",fontSize:20,fontWeight:"bold"}}),moving:!1,axis:0}),c=Ti.UI.createButton({backgroundImage:"newmenubutton.png",backgroundSelectedImage:"newmenubuttonselected.png",title:"",height:22,width:30,toggle:!1});l.setLeftNavButton(c),l.add(r),c.addEventListener("click",function(){a.fireEvent("menuClick")}),a.addEventListener("swipeToggle",function(){a.fireEvent("menuClick")}),a.addEventListener("swipe",function(){a.fireEvent("menuClick")}),a.addEventListener("swipeListen",function(){a.fireEvent("menuClick")});var u=Ti.UI.createWindow({barImage:"navbar.png",navBarHidden:!1});u.add(d);var s=Ti.UI.iPhone.createNavigationGroup({window:l});return a.add(s),r.addEventListener("itemSelected",function(e){d.showArticle(e.link),s.open(u)}),a}var RootWindow=require("ui/common/RootWindow");module.exports=ApplicationWindow;