function ApplicationWindow(e,t,i){i.trackScreen(t);var o=(require("services/rss"),require("ui/common/MasterView")),a=require("ui/common/DetailView"),n=(require("ui/common/Description"),Ti.UI.createWindow({backgroundColor:"#ffffff",navBarHidden:!0}),new o(e,t,i)),r=(new a,Ti.UI.createWindow({title:t,barImage:"navbar.png",translucent:!1,backgroundColor:"#e2e2e2",width:Ti.Platform.displayCaps.platformWidth,left:0,moving:!1,axis:0}));Ti.Platform.version>=7&&(r.titleControl=Ti.UI.createLabel({text:t,color:"white",font:{fontFamily:"HelveticaNeue-CondensedBold",fontSize:20,fontWeight:"bold"}}));var l=Ti.UI.createButton({backgroundImage:"newmenubutton.png",backgroundSelectedImage:"newmenubuttonselected.png",title:"",height:22,width:37,toggle:!1});r.setLeftNavButton(l),r.add(n);var d=Titanium.UI.iOS.createNavigationWindow({window:r});return l.addEventListener("click",function(){d.fireEvent("menuClick")}),d.addEventListener("swipeToggle",function(){d.fireEvent("menuClick")}),d.addEventListener("swipe",function(){d.fireEvent("menuClick")}),d.addEventListener("swipeListen",function(){d.fireEvent("menuClick")}),d}var RootWindow=require("ui/common/RootWindow");module.exports=ApplicationWindow;