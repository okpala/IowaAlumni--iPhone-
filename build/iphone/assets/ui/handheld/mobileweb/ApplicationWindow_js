function ApplicationWindow(){function e(){t.loadRssFeed({success:function(e){n.refreshRssTable(e)}})}var t=require("services/rss"),i=require("ui/common/MasterView"),o=require("ui/common/DetailView"),a=Ti.UI.createWindow({backgroundColor:"#fff"}),n=new i,r=new o,d=Ti.UI.createWindow({title:"RSS Reader",layout:"vertical"}),l=Ti.UI.createButton({backgroundImage:"images/refresh_icon.png",width:40,height:40,top:0,right:10});l.addEventListener("click",function(){e()}),d.add(l),d.add(n);var c=Ti.UI.createWindow();c.add(r);var u=Ti.UI.MobileWeb.createNavigationGroup({window:d});return a.add(u),n.addEventListener("itemSelected",function(e){r.showArticle(e.link),u.open(c)}),e(),a}module.exports=ApplicationWindow;