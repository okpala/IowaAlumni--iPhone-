function ApplicationWindow(){function e(){t.loadRssFeed({start:function(){r.show()},error:function(){r.hide()},success:function(e){n.refreshRssTable(e),r.hide()}})}var t=require("services/rss"),i=require("ui/common/MasterView"),o=require("ui/common/DetailView"),a=Ti.UI.createWindow({title:"RSS Reader",backgroundColor:"#fff",exitOnClose:!0,navBarHidden:!1,activity:{onCreateOptionsMenu:function(t){var i=t.menu,o=i.add({title:"Refresh"});o.setIcon("images/refresh_icon.png"),o.addEventListener("click",function(){e()})}}}),n=new i,r=Ti.UI.createActivityIndicator({message:"Loading...",color:"#fff"});return a.add(n),n.addEventListener("itemSelected",function(e){var t=new o,i=Ti.UI.createWindow({title:"View Article",navBarHidden:!1,backgroundColor:"#fff"});i.add(t);var a;i.addEventListener("open",function(){a=Ti.UI.createActivityIndicator({location:Ti.UI.ActivityIndicator.STATUS_BAR,type:Ti.UI.ActivityIndicator.DETERMINANT,message:"Loading article..."}),a.show()}),t.addEventListener("articleLoaded",function(){a.hide()}),t.showArticle(e.link),i.open()}),a.addEventListener("open",function(){e()}),a}module.exports=ApplicationWindow;