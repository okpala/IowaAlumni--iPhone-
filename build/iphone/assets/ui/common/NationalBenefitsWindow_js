function NationalBenefitsWindow(){var e=new Feed,t=Ti.UI.createView(),i=Ti.UI.createView({backgroundImage:"gray-broder.png",height:90,width:316,top:0,left:2}),o=Ti.UI.createLabel({text:"As a UIAA member, you gain access to a variety of exclusive benefits that show our appreciation for your support of the UIAA.",textAlign:"left",left:10,width:300,top:10,font:{fontFamily:"HelveticaNeue-Light",fontSize:14,fontWeight:"bold"}});i.add(o);var a=Ti.UI.createTableView({height:"auto",top:70}),n=Ti.UI.createLabel({text:"IC benefits",left:250,top:50,color:"blue",font:{fontFamily:"HelveticaNeue-Light",fontSize:14,fontWeight:"bold"}});n.addEventListener("click",function(){(new MapWindow).open()});for(var r=new GetFeed(e.nationalDiscountFeed()),d=[],l=0;r.length-1>=l;l++){if(0==l%2)var c=Ti.UI.createTableViewRow({height:"auto",bottom:10});else var c=Ti.UI.createTableViewRow({height:"auto",backgroundColor:"#cccccc",bottom:10});var s=Ti.UI.createLabel({text:r[l].title,textAlign:"left",height:20,top:10,left:10,font:{fontFamily:"Helvetica-Bold",fontSize:16,fontWeight:"normal"}}),u=Ti.UI.createLabel({text:r[l].description,textAlign:"left",left:10,top:31,font:{fontFamily:"HelveticaNeue-Light",fontSize:12,fontWeight:"bold"}});c.add(s),c.add(u),d.push(c)}a.addEventListener("click",function(e){new WebView(r[e.index].link)}),a.setData(d),i.add(n),t.add(a),t.add(i);var g=new ApplicationWindow("National Member Benefits",t);return g}var MapWindow=require("ui/common/MapWindow"),ApplicationWindow=require("ui/common/ApplicationWindow"),GetFeed=require("ui/common/GetFeed"),WebView=require("ui/common/WebView"),Feed=require("ui/common/Feed");module.exports=NationalBenefitsWindow;