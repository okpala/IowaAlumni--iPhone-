function IIBIntroRow(){var e=Ti.UI.createTableView({separatorColor:"d5d5d5",backgroundColor:"ffffff",height:"auto",width:300,left:10,top:10,bottom:0,padding:0,borderRadius:5,borderColor:"#d5d5d5",borderWidth:1,scrollable:!1}),t=Ti.UI.createTableViewRow({selectionStyle:"none",height:50});e.height=t.height;var i=[];i.push(t),e.setData(i);var a=Ti.UI.createTableViewRow({hasChild:!0,height:e.height+10,padding:0,top:0,bottom:0,layout:"vertical",backgroundColor:"e2e2e2"});return a.rightImage=null,a.backgroundSelectedImage=null,a.backgroundFocusImage=null,a.add(e),introLabel=getText(),t.add(introLabel),a}function getText(){var e=Ti.UI.createLabel({text:"The official blog of the University of Iowa Alumni Association.",left:10,top:10,textAlign:"left",width:290,color:"#000000",font:{fontFamily:"HelveticaNeueBold",fontSize:12,fontWeight:"bold"}});return e}var WebView=require("ui/common/WebView");module.exports=IIBIntroRow;