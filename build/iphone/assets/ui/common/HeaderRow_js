function HeaderRow(e,t,i){var n=Ti.UI.createTableView({separatorColor:"e2e2e2",backgroundColor:"e2e2e2",height:"auto",width:300,left:10,top:10,bottom:0,padding:0,scrollable:!1}),o=Ti.UI.createTableViewRow({backgroundImage:"gold.png",text:"hello",height:50,selectedBackgroundImage:"blue.png"});o.addEventListener("click",function(){new WebView(e.hlink),t.trackEvent({category:"Events",action:"click",label:"An Event in the "+i+"'s Window - "+e.hlink,value:1})}),n.height=o.height;var a=[];a.push(o),n.setData(a);var r=Ti.UI.createTableViewRow({hasChild:!0,height:n.height+8,padding:0,top:0,bottom:0,link:e.hlink,layout:"vertical",selectionStyle:"none",backgroundColor:"e2e2e2"});return r.rightImage=null,r.backgroundSelectedImage=null,r.backgroundFocusImage=null,r.add(n),datebl=getpubDateLabel(e.pubDate),o.add(datebl),r}function getpubDateLabel(e){var t=Ti.UI.createLabel({text:e,left:15,bottom:15,textAlign:"left",width:200,height:25,color:"#5c4e1a",font:{fontFamily:"Helvetica-Bold",fontSize:20,fontWeight:"normal"}});return t}var DateObject=require("ui/common/DateObject"),EditText=require("ui/common/EditText"),WebView=require("ui/common/WebView");module.exports=HeaderRow;