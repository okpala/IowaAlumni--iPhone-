function Ad(e){var n=Ti.UI.createTableViewRow({hasChild:!0,height:70,padding:0,top:0,bottom:0,layout:"vertical",backgroundColor:"e2e2e2",borderRadius:.5,selectionStyle:"none"});n.rightImage=null,n.backgroundSelectedImage=null,n.backgroundFocusImage=null;var i=Ti.UI.createView({backgroundColor:"transparent",height:300,width:300,left:10,top:-5,bottom:0,padding:0,borderRadius:5}),t=Ti.UI.createImageView({image:e.ad,width:300,height:70,hires:!0,top:10});return i.add(t),n.add(i),n.addEventListener("click",function(){new WebView(e.link)}),n}var WebView=require("ui/common/WebView");module.exports=Ad;