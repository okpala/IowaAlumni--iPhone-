function Ad(e,n,i){var t=Ti.UI.createTableViewRow({hasChild:!0,height:70,padding:0,top:0,bottom:0,layout:"vertical",backgroundColor:"e2e2e2",borderRadius:.5,selectionStyle:"none"});t.rightImage=null,t.backgroundSelectedImage=null,t.backgroundFocusImage=null;var o=Titanium.UI.createView({backgroundColor:"transparent",height:300,width:300,left:10,top:-5,bottom:0,padding:0,borderRadius:5}),a=Ti.UI.createImageView({image:e.ad,defaultImage:"loader480x120.png",width:300,height:70,hires:!0,top:10});return o.add(a),t.add(o),t.addEventListener("click",function(){new WebView(e.link),n.trackEvent({category:"Ads",action:"click",label:"An Ad in the "+i+"'s Window - "+e.ad,value:1})}),t}var WebView=require("ui/common/WebView");module.exports=Ad;