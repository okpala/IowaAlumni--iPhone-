function IAMIntroRow(){var e=Ti.UI.createTableView({separatorColor:"d5d5d5",backgroundColor:"ffffff",height:"auto",width:300,left:10,top:10,bottom:0,padding:0,borderRadius:5,borderColor:"#d5d5d5",borderWidth:1,scrollable:!1}),t=Ti.UI.createTableViewRow({selectionStyle:"none",height:80}),i=Ti.UI.createImageView({image:"http://www.iowalum.com/magazine/new/images/magfan.jpg",top:10,left:200,width:80,height:55});e.height=t.height;var a=[];a.push(t),e.setData(a);var o=Ti.UI.createTableViewRow({hasChild:!0,height:e.height+10,padding:0,top:0,bottom:0,layout:"vertical",selectionStyle:"none",backgroundColor:"e2e2e2"});return o.rightImage=null,o.backgroundSelectedImage=null,o.backgroundFocusImage=null,o.add(e),introLabel=getText(),t.add(introLabel),t.add(i),o}function getText(){var e=Ti.UI.createLabel({text:"Our award winning bimonthly magazine published by the University of Iowa Alumni Association. ",left:10,top:10,textAlign:"left",width:200,color:"#000000",font:{fontFamily:"HelveticaNeueBold",fontSize:12,fontWeight:"bold"}});return e}var WebView=require("ui/common/WebView");module.exports=IAMIntroRow;