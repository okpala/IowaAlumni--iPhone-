function Description(e){this.description=e}Description.prototype.getImage=function(){var e=/(img|src)=("|')[^"'>]+/,t=e.exec(this.description),i="";if(null!=t)if(t.length>1){i=t[0],i=i.substring(5,i.length);var n=/-(\d{3})x(\d{3}).jpg/;i=i.replace(n,".jpg"),i=i.replace(/ /g,"%20")}else i=null;else i=null;return i},Description.prototype.getImageMagazine=function(){var e=/(img|src)=("|')[^"'>]+/,t=e.exec(this.description),i=t.substring(t.length-52,52);return i},Description.prototype.getDescription=function(){var e=this.description;e=e.replace(/(\r\n|\n|\r|\t)/gm," "),e=e.replace("&#8217;","'");var t=/(<([^>]+)>)/gi;e=e.replace(t,"");var i=/&[^;]+?;/;e=e.replace(i,"");for(var n=e.substr(0,1);" "==n||"	"==n;)e=e.substring(e.length-1,1),n=e.substr(0,1);return e},module.exports=Description;