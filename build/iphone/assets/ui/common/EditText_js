function EditText(e){e=e.replace(/(\r\n|\n|\r|\t)/gm,""),e=e.replace("&#8217;","'");var t=/(<([^>]+)>)/gi;e=e.replace(t,"");var i=/&[^;]+?;/;e=e.replace(i,"");for(var n=e.substr(0,1);" "==n||"	"==n;)e=e.substring(e.length-1,1),n=e.substr(0,1);newText=e}var newText="";EditText.prototype.adjustedText=function(){return newText},module.exports=EditText;