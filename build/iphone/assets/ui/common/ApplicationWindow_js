function ApplicationWindow(e,n){var i=Ti.UI.createWindow({backgroundColor:"#e2e2e2",navBarHidden:!0}),t=Ti.UI.createView({backgroundColor:"#000",top:0,height:20});i.add(t);var a=Ti.UI.createWindow({title:e,navBarHidden:!1,barImage:"navbar.png",titleControl:Ti.UI.createLabel({text:e,color:"white",font:{fontFamily:"HelveticaNeue-CondensedBold",fontSize:20,fontWeight:"bold"}}),moving:!1,axis:0}),r=Ti.UI.createButton({backgroundImage:"newmenubutton.png",backgroundSelectedImage:"newmenubuttonselected.png",title:"",height:22,width:37,toggle:!1});a.setLeftNavButton(r),a.add(n),r.addEventListener("click",function(){i.fireEvent("menuClick")}),i.addEventListener("swipeToggle",function(){i.fireEvent("menuClick")}),i.addEventListener("swipe",function(){i.fireEvent("menuClick")}),i.addEventListener("swipeListen",function(){i.fireEvent("menuClick")});var o=Ti.UI.iPhone.createNavigationGroup({window:a});return i.add(o),i}module.exports=ApplicationWindow;