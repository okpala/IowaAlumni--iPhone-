function ApplicationWindow(e,n){var i=(Ti.UI.createView({backgroundColor:"#000",top:0,height:20}),Ti.UI.createWindow({title:e,backgroundColor:"#e2e2e2",width:Ti.Platform.displayCaps.platformWidth,left:0,barImage:"navbar.png",translucent:!1,moving:!1,axis:0})),t=Ti.UI.createButton({backgroundImage:"newmenubutton.png",backgroundSelectedImage:"newmenubuttonselected.png",title:"",height:22,width:37,toggle:!1});i.setLeftNavButton(t),i.add(n);var o=Titanium.UI.iOS.createNavigationWindow({window:i});return t.addEventListener("click",function(){o.fireEvent("menuClick")}),o.addEventListener("swipeToggle",function(){o.fireEvent("menuClick")}),o.addEventListener("swipe",function(){o.fireEvent("menuClick")}),o.addEventListener("swipeListen",function(){o.fireEvent("menuClick")}),o}module.exports=ApplicationWindow;