function ClubsandWatchesScrollWindow(e,t,i,n){for(var o=0,a=Ti.UI.createView({backgroundColor:"#fff"}),r=60,l=e[0].state,d=0,s=Ti.UI.createImageView({image:"slide_right.png",bottom:3,right:5,width:54,height:54,zIndex:5}),c=Ti.UI.createImageView({image:"slide_left.png",bottom:3,left:5,width:54,height:54,zIndex:5}),u=["Iowa Clubs","Game Watch Locations"],w=["dots2_1.png","dots2_2.png"],m=[],g=0;2>g;g++){var T=Titanium.UI.createView({}),b=Ti.UI.createLabel({text:u[g],font:{fontFamily:"HelveticaNeue-Light",fontSize:20,fontWeight:"bold"},top:15}),h=Ti.UI.createImageView({image:w[g],top:45,height:10});T.add(b),T.add(h),m[g]=T}var f=Titanium.UI.createScrollableView({width:Ti.UI.FILL,height:r,views:m,bottom:0,backgroundColor:"#ccc",contentWidth:Ti.UI.FILL,contentHeight:100});a.add(s),a.add(c),s.hide(),c.hide();var v=new ClubsWindow(e,t,i,d);a.add(v),a.add(f);var p=[a,v,n];f.addEventListener("scrollend",function(e){0==e.currentPage&&1==o?(p[0].remove(p[2]),o=0):1==e.currentPage&&0==o&&(p[0].add(p[2]),o=1)}),f.addEventListener("scroll",function(){c.hide(),s.hide()}),f.addEventListener("touchstart",function(){0==o?c.show():s.show()}),f.addEventListener("touchend",function(){0==o?c.hide():s.hide()});var R=new NavigateWindow(l,a);return R}var NavigateWindow=require("ui/common/NavigateWindow"),ClubsWindow=require("ui/common/ClubsWindow"),GameWatchWindow=require("ui/common/GameWatchWindow");module.exports=ClubsandWatchesScrollWindow;