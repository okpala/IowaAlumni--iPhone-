function GetFeed(e){var t=[];xmlDoc=loadXMLDoc(e);var i=xmlDoc.getElementsByTagName("item"),a=i.item(o);if(e==Feeds.articleOfTheWeekFeed())t.push({title:getRssText(a,"title"),url:getRssText(a,"link"),image:getRssText(a,"image"),description:getRssText(a,"description"),pubDate:getRssText(a,"pubDate")});else if(e==Feeds.todayDateFeed())t.push({date:getRssText(a,"date")});else if(e==Feeds.passwordFeed())t.push({pass:getRssText(a,"pass"),pass2:getRssText(a,"pass2")});else for(var o=0;i.length>o;o++){var a=i.item(o);e==Feeds.gameWatchFeed()?t.push({state:getRssText(a,"state"),club:getRssText(a,"club"),place:getRssText(a,"place"),phone:getRssText(a,"phone"),latitude:getRssText(a,"latitude"),longitude:getRssText(a,"longitude"),street:getRssText(a,"street")}):e==Feeds.nationalDiscountFeed()?t.push({title:getRssText(a,"title"),link:getRssText(a,"link"),description:getRssText(a,"description")}):"http://www.iowalum.com/mobile-app/events_category_feed.cfm"==e?t.push({category:getRssText(a,"category")}):e==Feeds.mobileAlertsFeed()?t.push({title:getRssText(a,"title"),header:getRssText(a,"header"),url:getRssText(a,"link"),image:getRssText(a,"image"),description:getRssText(a,"description"),pubDate:getRssText(a,"pubDate")}):e==Feeds.staticaAdFeed()?t.push({ad:getRssText(a,"ad"),adUrl:getRssText(a,"adUrl")}):e==Feeds.mobileAlertsFeed()?t.push({title:getRssText(a,"title"),header:getRssText(a,"header"),url:getRssText(a,"link"),image:getRssText(a,"image"),description:getRssText(a,"description"),pubDate:getRssText(a,"pubDate")}):e==Feeds.todayEventsFeed()?t.push({snl:getRssText(a,"snl"),place:getRssText(a,"place"),title:getRssText(a,"title"),url:getRssText(a,"link"),description:getRssText(a,"description"),pubDate:getRssText(a,"pubDate")}):e==Feeds.clubsFeed()?t.push({state:getRssText(a,"state"),leader:getRssText(a,"leader"),city:getRssText(a,"city"),web:getRssText(a,"web"),email:getRssText(a,"email"),phone:getRssText(a,"phone")}):e==Feeds.adFeed()?t.push({ad:getRssText(a,"ad"),link:getRssText(a,"link")}):e==Feeds.sliderImagesFeed()?t.push({url:getRssText(a,"url")}):t.push({company:getRssText(a,"company"),discount:getRssText(a,"discount"),latitude:getRssText(a,"latitude"),longitude:getRssText(a,"longitude"),street:getRssText(a,"street")})}return t}function loadXMLDoc(e){var t=Ti.Network.createHTTPClient();return t.open("GET",e,!1),t.send(),t.responseXML}var Feed=require("ui/common/Feed"),Feeds=new Feed,osname=Ti.Platform.osname,RSS_URL="mobileweb"===osname?"/feed.xml":Feeds.magazineFeed(),getRssText=function(e,t){return"mobileweb"===osname?e.getElementsByTagName(t).item(0).textContent:e.getElementsByTagName(t).item(0).text};module.exports=GetFeed;