function ImageCaching(e,t){var i=t.split("/");i=i[i.length-1];var a=Ti.Filesystem.getFile(Ti.Filesystem.applicationDataDirectory,e,i),o="";if(a.exists())o=a.nativePath,a=null;else{var n=Ti.Filesystem.getFile(Ti.Filesystem.applicationDataDirectory,e);n.exists()||n.createDirectory();var r=Ti.Network.createHTTPClient();r.onload=function(){200==r.status&&(a.write(r.responseData),o=a.nativePath,a=null)},r.open("GET",t),r.send()}return o}module.exports=ImageCaching;