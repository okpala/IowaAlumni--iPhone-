function ImageCaching(e,t){var i=t.split("/");i=i[i.length-1];var o=Ti.Filesystem.getFile(Ti.Filesystem.applicationDataDirectory,e,i),n="";if(o.exists())n=o.nativePath,o=null;else{var a=Ti.Filesystem.getFile(Ti.Filesystem.applicationDataDirectory,e);a.exists()||a.createDirectory();var r=Ti.Network.createHTTPClient();r.onload=function(){200==r.status&&(o.write(r.responseData),n=o.nativePath,o=null)},r.open("GET",t),r.send()}return n}module.exports=ImageCaching;