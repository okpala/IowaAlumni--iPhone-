function ContactUsWindow(e,t){t.trackScreen(e);var i=Ti.UI.createView({separatorColor:"d5d5d5",backgroundColor:"ffffff",height:160,width:300,left:10,top:10,bottom:0,padding:0,borderRadius:5,borderColor:"#d5d5d5",borderWidth:1}),n=Ti.UI.createView({separatorColor:"d5d5d5",backgroundColor:"ffffff",height:160,width:300,left:10,top:180,bottom:0,padding:0,borderRadius:5,borderColor:"#d5d5d5",borderWidth:1}),o=Ti.UI.createView({top:0,contentWidth:320,contentHeight:420,showVerticalScrollIndicator:!1,showHorizontalScrollIndicator:!1}),a=new StaticAd(14,392,t,e),r=0,l=Ti.UI.createLabel({text:"Contact Us",top:10}),d=Ti.UI.createLabel({text:"Levitt Center",top:30});r=d.top;var c=Ti.UI.createView({width:67,top:44});d.addEventListener("click",function(){new WebView("http://www.iowalum.com/about/levitt.cfm"),t.trackEvent({category:"General Information",action:"click",label:"UIAA About Us Site",value:1})});var s=Ti.UI.createLabel({text:"P.O. Box 1970".concat("\n").concat("Iowa City, IA 52244-1970"),top:r+15}),u=Ti.UI.createLabel({text:"Phone: 319/335-3294".concat("\n").concat("Toll Free: 800/469-2586").concat("\n").concat("FAX: 319/335-1079"),top:85}),w=Ti.UI.createLabel({text:"alumni@uiowa.edu",top:128}),m=Ti.UI.createView({width:99,top:142});w.addEventListener("click",function(){var e=Ti.UI.createEmailDialog();e.toRecipients=["alumni@uiowa.edu"];var i=Ti.Filesystem.getFile("cricket.wav");e.addAttachment(i),e.open(),t.trackEvent({category:"General Information",action:"click",label:"UIAA Email Address",value:1})});var g=Ti.UI.createLabel({text:"Social Networks",left:10,top:10}),b=new SocialMediaIcons,T=b.facebook(37,55,t),h=b.twitter(37,115,t),f=b.foursquare(37,175,t),v=b.linkedIn(97,55,t),p=b.pinterest(97,115,t),I=b.instagram(97,175,t);w.font=u.font=s.font=d.font={fontFamily:"HelveticaNeue-Light",fontSize:12,fontWeight:"bold"},g.font=l.font={fontFamily:"Helvetica-Bold",fontSize:16,fontWeight:"normal"},w.textAlign=u.textAlign=s.textAlign=d.textAlign=g.textAlign=l.textAlign="left",I.width=p.width=v.width=f.width=h.width=T.width=48,I.height=p.height=v.height=f.height=h.height=T.height=48,m.backgroundColor=c.backgroundColor=w.color=d.color="blue",m.height=c.height=1,g.left=l.left=m.left=c.left=w.left=u.left=s.left=d.left=10,i.add(l),i.add(d),i.add(c),i.add(s),i.add(u),i.add(w),i.add(m),n.add(g),n.add(T),n.add(h),n.add(f),n.add(v),n.add(p),n.add(I),o.add(n),o.add(i),o.add(a);var R=new ApplicationWindow(e,o);return R}var ApplicationWindow=require("ui/common/ApplicationWindow"),WebView=require("ui/common/WebView"),SocialMediaIcons=require("ui/common/SocialMediaIcons"),StaticAd=require("ui/common/StaticAd");module.exports=ContactUsWindow;