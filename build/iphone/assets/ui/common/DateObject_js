function DateObject(e){this.pubDate=e}DateObject.prototype.dateString=function(){var e=new Date(this.pubDate),t=["Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec"],i=e.getDate();10>i&&(i="0"+i);var n=e.getMonth(),a=t[n]+" "+i,o="AM",r=e.getHours(),d=e.getMinutes();r>=12&&(o="PM"),r>12&&(r-=12),10>d&&(d="0"+(""+d)),10>r&&(r="0"+(""+r));var s=r+":"+d+o;return a+" "+s},DateObject.prototype.magazineDateString=function(){var e=new Date(this.pubDate),t=["Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec"],i=e.getMonth(),n=t[i];return n+" "+e.getYear()},DateObject.prototype.prettyDate=function(){var e=["Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec"],t=new Date(this.pubDate);new Date;var i=(new Date).getTime()-t.getTime(),n=Math.floor(i/86400);if(isNaN(n)||0>n)return"";if(n>=31){var a=t.getFullYear(),o=e[t.getMonth()],r=t.getMonth()+1;10>r&&(r="0"+r);var d=t.getDate();return 10>d&&(d="0"+d),d+" "+o+" "+a}return 0==n&&(60>i&&"just now"||120>i&&"1 minute ago"||3600>i&&Math.floor(i/60)+" minutes ago"||7200>i&&"1 hour ago"||86400>i&&"about "+Math.floor(i/3600)+" hours ago")||1==n&&"Yesterday"||7>n&&n+" days ago"||31>n&&Math.ceil(n/7)+" week"+(1==Math.ceil(n/7)?"":"s")+" ago"},module.exports=DateObject;