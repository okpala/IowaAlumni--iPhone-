function DateObject(e){this.pubDate=e}DateObject.prototype.dateString=function(){var e=new Date(this.pubDate),t=new Array("Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec"),i=e.getDate();10>i&&(i="0"+i);var n=e.getMonth(),o=t[n]+" "+i,a="AM",r=e.getHours(),l=e.getMinutes();r>=12&&(a="PM"),r>12&&(r-=12),10>l&&(l="0"+l.toString()),10>r&&(r="0"+r.toString());var d=r+":"+l+a;return o+" "+d},DateObject.prototype.magazineDateString=function(){var e=new Date(this.pubDate),t=new Array("Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec"),i=e.getMonth(),n=t[i];return n+" "+e.getYear()},DateObject.prototype.prettyDate=function(){var e=["Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec"],t=new Date(this.pubDate),i=(new Date,(new Date).getTime()-t.getTime()),n=Math.floor(i/86400);if(isNaN(n)||0>n)return"";if(n>=31){var o=t.getFullYear(),a=e[t.getMonth()],r=t.getMonth()+1;10>r&&(r="0"+r);var l=t.getDate();return 10>l&&(l="0"+l),l+" "+a+" "+o}return 0==n&&(60>i&&"just now"||120>i&&"1 minute ago"||3600>i&&Math.floor(i/60)+" minutes ago"||7200>i&&"1 hour ago"||86400>i&&"about "+Math.floor(i/3600)+" hours ago")||1==n&&"Yesterday"||7>n&&n+" days ago"||31>n&&Math.ceil(n/7)+" week"+(1==Math.ceil(n/7)?"":"s")+" ago"},module.exports=DateObject;