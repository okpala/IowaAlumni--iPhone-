function ClubsWindow(e,t,i,n){for(var o=Ti.UI.createView({backgroundColor:"#e2e2e2",top:n,bottom:60}),a=Ti.UI.createTableView({height:"auto",top:0}),r=[],l=0,d=0;d<=t.length-1;d++){if(l%2==0)var s=Ti.UI.createTableViewRow({city:t[d].city,height:"auto",selectionStyle:"none",index:d,bottom:10});else var s=Ti.UI.createTableViewRow({city:t[d].city,height:"auto",selectionStyle:"none",backgroundColor:"#cccccc",bottom:10});var c=Ti.UI.createLabel({text:t[d].city,textAlign:"left",height:20,top:10,left:10,font:{fontFamily:"Helvetica-Bold",fontSize:16,fontWeight:"normal"}});s.add(c);var u=Ti.UI.createLabel({text:t[d].leader,textAlign:"left",left:10,top:31,font:{fontFamily:"HelveticaNeue-Light",fontSize:12,fontWeight:"bold"}});s.add(u);var w=46,m=10;if("NA"!=t[d].phone){var T=Ti.UI.createLabel({text:t[d].phone,textAlign:"left",left:10,top:w,font:{fontFamily:"HelveticaNeue-Light",fontSize:12,fontWeight:"bold"}});s.add(T),w+=15}if("NA"!=t[d].email){var g=Ti.UI.createLabel({text:t[d].email,textAlign:"left",color:"blue",index:d,left:10,top:w,font:{fontFamily:"HelveticaNeue-Light",fontSize:12,fontWeight:"bold"}});g.addEventListener("click",function(e){var n=Ti.UI.createEmailDialog();n.toRecipients=[t[e.index].email];var o=Ti.Filesystem.getFile("cricket.wav");n.addAttachment(o),n.open(),i.trackEvent({category:"Clubs",action:"click",label:t[index].city+" "+t[index].email,value:1})}),s.add(g),w+=15}if("NA"!=t[d].web){var b=Ti.UI.createLabel({text:t[d].web,textAlign:"left",left:10,top:w,color:"blue",font:{fontFamily:"HelveticaNeue-Light",fontSize:12,fontWeight:"bold"}});b.addEventListener("click",function(e){new WebView(t[e.index].web),i.trackEvent({category:"Clubs",action:"click",label:t[e.index].city+" "+t[e.index].web,autoLink:Titanium.UI.AUTOLINK_URLS,value:1})}),s.add(b),w+=20}if("NA"!=t[d].phone){var f=Ti.UI.createButton({backgroundImage:"call.png",width:35,height:35,top:w,left:m,buttonid:d});m+=40,s.add(f),f.addEventListener("click",function(e){var n=t[e.source.buttonid].phone.replace(/(\|H: |C: |W: )/gm,"");n=n.replace(/(-)/gm,""),n=n.replace(/(H: )/gm,""),Titanium.Platform.openURL("tel:"+n),i.trackEvent({category:"Clubs",action:"click",label:t[e.source.buttonid].city+" "+t[e.source.buttonid].phone,value:1})})}if("NA"!=t[d].email){var h=Ti.UI.createButton({backgroundImage:"mail.png",width:35,height:35,top:w,left:m,buttonid:d});m+=40,h.addEventListener("click",function(e){var n=Ti.UI.createEmailDialog();n.toRecipients=[t[e.source.buttonid].email];var o=Ti.Filesystem.getFile("cricket.wav");n.addAttachment(o),n.open(),i.trackEvent({category:"Clubs",action:"click",label:t[e.source.buttonid].city+" "+t[e.source.buttonid].email,value:1})}),s.add(h)}if("NA"!=t[d].web){var v=Ti.UI.createButton({backgroundImage:"web.png",width:35,height:35,top:w,left:m,buttonid:d});v.addEventListener("click",function(e){new WebView(t[e.source.buttonid].web),i.trackEvent({category:"Clubs",action:"click",label:t[e.source.buttonid].city+" "+t[e.source.buttonid].web,autoLink:Titanium.UI.AUTOLINK_URLS,value:1})}),s.add(v)}l++,r.push(s)}return r=addRows(d,r,!0),a.setData(r),o.add(a),o}function addRows(e,t,i){if(1==e&&1==i){var n=Ti.UI.createTableViewRow({height:100,selectionStyle:"none",backgroundColor:"#cccccc",bottom:10});t.push(n);var n=Ti.UI.createTableViewRow({height:100,selectionStyle:"none",bottom:10});t.push(n);var n=Ti.UI.createTableViewRow({height:100,selectionStyle:"none",backgroundColor:"#cccccc",bottom:10});t.push(n)}else if(1==e&&0==i){var n=Ti.UI.createTableViewRow({height:100,selectionStyle:"none",backgroundColor:"#cccccc",bottom:10});t.push(n)}else if(2==e&&1==i){var n=Ti.UI.createTableViewRow({height:100,selectionStyle:"none",bottom:10});t.push(n);var n=Ti.UI.createTableViewRow({height:100,selectionStyle:"none",backgroundColor:"#cccccc",bottom:10});t.push(n)}else if(3==e&&1==i){var n=Ti.UI.createTableViewRow({height:100,selectionStyle:"none",backgroundColor:"#cccccc",bottom:10});t.push(n)}return t}var GetFeed=require("ui/common/GetFeed"),WebView=require("ui/common/WebView"),ApplicationWindow=require("ui/common/ApplicationWindow"),StaticAd=require("ui/common/StaticAd"),Feed=require("ui/common/Feed");module.exports=ClubsWindow;