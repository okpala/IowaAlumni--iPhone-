function ClubsWindow(e){var t=new Feed,n=Ti.UI.createView(),i=Ti.UI.createLabel({text:"Want to connect with fellow UI grads, need a place to watch the next game with fellow Hawkeye fans? IOWA clubs have you coveredâ€”find a location near you!",textAlign:"left",left:10,width:300,top:10,font:{fontFamily:"HelveticaNeue-Light",fontSize:14,fontWeight:"bold"}});n.add(i);var a=Ti.UI.createTableView({height:"auto",bottom:70,top:145}),o=Ti.UI.createImageView({image:"https://www.iowalum.com/mobile/clubs.png",top:85});n.add(o);for(var r=new GetFeed(t.clubsFeed()),d=new GetFeed(t.gameWatchFeed()),s=[],l=0,m=0;d.length-1>=m;m++)if(0==m||d[m-1].state!=d[m].state&&0!=m){if(0==l%2)var w=Ti.UI.createTableViewRow({text:d[m].state,height:50});else var w=Ti.UI.createTableViewRow({text:d[m].state,backgroundColor:"#cccccc",height:50});var c=Ti.UI.createLabel({text:d[m].state,textAlign:"center",font:{fontFamily:"Helvetica-Bold",fontSize:16,fontWeight:"normal"}});w.add(c),s.push(w),l++}a.setData(s),n.add(a),a.addEventListener("click",function(e){var t=getStateList(d,r,e.row.text);new GameWatchWindow(t[0],t[1]).open()});var u=new StaticAd(11,395);n.add(u);var g=new ApplicationWindow(e,n);return g}function getStateList(e,t,n){for(var i=[],a=[],o=[],r=0;e.length-1>=r;r++)e[r].state==n&&a.push(e[r]);i.push(a);for(var r=0;t.length-1>=r;r++)t[r].state.toUpperCase()==n&&o.push(t[r]);return i.push(o),i}var GetFeed=require("ui/common/GetFeed"),GameWatchWindow=require("ui/common/GameWatchWindow"),WebView=require("ui/common/WebView"),ApplicationWindow=require("ui/common/ApplicationWindow"),StaticAd=require("ui/common/StaticAd"),Feed=require("ui/common/Feed");module.exports=ClubsWindow;