function NavigateWindow(e,t){var i=Ti.UI.createWindow({backgroundColor:"#e2e2e2",navBarHidden:!0}),o=Ti.UI.createView({backgroundColor:"#000",top:0,height:20});i.add(o);var a=Ti.UI.createWindow({title:e,navBarHidden:!1,barImage:"navbar.png",moving:!1,axis:0}),n=Ti.UI.createButton({height:26,width:15,left:10,backgroundImage:"back.png",font:{fontFamily:"Helvetica Neue",fontSize:14,fontWeight:"bold"},toggle:!1});a.setLeftNavButton(n),a.add(t),n.addEventListener("click",function(){i.close()});var r=Ti.UI.iPhone.createNavigationGroup({window:a});return i.add(r),i}module.exports=NavigateWindow;