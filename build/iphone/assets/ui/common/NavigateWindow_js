function NavigateWindow(e,t){var i=(Ti.UI.createWindow({backgroundColor:"#e2e2e2",navBarHidden:!0}),Ti.UI.createView({backgroundColor:"#000",top:0,height:20}),Ti.UI.createWindow({title:e,navBarHidden:!1,translucent:!1,barImage:"navbar.png",moving:!1,axis:0})),o=Ti.UI.createButton({height:26,width:15,left:10,backgroundImage:"back.png",font:{fontFamily:"Helvetica Neue",fontSize:14,fontWeight:"bold"},toggle:!1});i.setLeftNavButton(o),i.add(t);var a=Titanium.UI.iOS.createNavigationWindow({window:i});return o.addEventListener("click",function(){a.close()}),a}module.exports=NavigateWindow;