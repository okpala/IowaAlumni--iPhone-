&1==i){var a=Ti.UI.createTableViewRow({height:100,selectionStyle:"none",backgroundColor:"#cccccc",bottom:10});t.push(a)}return t}var WebView=require("ui/common/WebView"),EditText=require("ui/common/EditText");module.exports=GameWatchWindow;