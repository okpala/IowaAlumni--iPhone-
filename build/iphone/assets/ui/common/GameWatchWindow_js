function GameWatchWindow(e,t){var i=e[0].state,a=Ti.UI.createWindow({backgroundColor:"#e2e2e2",navBarHidden:!0}),o=Ti.UI.createView({backgroundColor:"#000",top:0,height:20}),n=Ti.UI.createWindow({title:i,navBarHidden:!1,barImage:"navbar.png",moving:!1,axis:0}),r=Ti.UI.createButton({height:26,width:15,backgroundImage:"back.png",font:{fontFamily:"Helvetica Neue",fontSize:14,fontWeight:"bold"},toggle:!1});n.setLeftNavButton(r),r.addEventListener("click",function(){I.close(),a.close()});var d=Ti.UI.iPhone.createNavigationGroup({window:n});if(a.add(d),7>Ti.Platform.version)var l=43;else var l=63;for(var c=Ti.UI.createView({top:l,backgroundColor:"#ffffff",navBarHidden:!0}),u=[],s=0;e.length-1>=s;s++)u.push(Ti.Map.createAnnotation({latitude:e[s].latitude,longitude:e[s].longitude,title:e[s].place,subtitle:e[s].street,pincolor:Ti.Map.ANNOTATION_RED,animate:!0}));var g=Ti.Map.createView({mapType:Ti.Map.STANDARD_TYPE,region:{latitude:e[0].latitude,longitude:e[0].longitude,latitudeDelta:.01,longitudeDelta:.01},animate:!0,regionFit:!0,userLocation:!0,height:200,annotations:u,top:0});g.addEventListener("loading",function(){g.setLocation({latitude:e[0].latitude,longitude:e[0].longitude,latitudeDelta:.01,longitudeDelta:.01})}),g.addEventListener("postlayout",function(){g.setLocation({latitude:e[0].latitude,longitude:e[0].longitude,latitudeDelta:.01,longitudeDelta:.01})});for(var m=Ti.UI.createTableView({height:"auto",top:200}),h=[],w=0,s=0;u.length-1>=s;s++){if(0==w%2)var f=Ti.UI.createTableViewRow({club:e[s].club,latitude:e[s].latitude,longitude:e[s].longitude,height:"auto",bottom:10});else var f=Ti.UI.createTableViewRow({club:e[s].club,latitude:e[s].latitude,longitude:e[s].longitude,height:"auto",backgroundColor:"#cccccc",bottom:10});var p=Ti.UI.createLabel({text:e[s].club,textAlign:"left",height:20,top:10,left:10,font:{fontFamily:"Helvetica-Bold",fontSize:16,fontWeight:"normal"}}),b=Ti.UI.createLabel({text:e[s].place,textAlign:"left",left:10,top:31,height:14,font:{fontFamily:"He