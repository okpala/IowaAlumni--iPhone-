function MasterView(e){function t(){n(),i()}function i(){c.setContentInsets({top:60},{animated:!0}),c.reloading=!1,c.updateLabelText("Refreshing..."),setTimeout(o,700)}function o(){c.setContentInsets({top:0},{animated:!0}),c.updateDateText("Last Updated: "+(new FormatDate).getDate()),c.hideActInd(),c.updateLabelText("Pull down to refresh...")}function a(t){if("[object Array]"===Object.prototype.toString.apply(t)){for(var i=[],o=[],a=!1,n=0,d=0,s=0,u=0,g=new GetFeed(r.adFeed()),m="",h=0;t.length>h;h++){var w=new Post(t[h]);if(0==h&&e==r.iowaInsiderFeed()){var f=new IIBIntroRow;i.push(f)}if(0==h&&e==r.magazineFeed()){var f=new IAMIntroRow;i.push(f)}if(0!=d&&0==d%3&&3>u&&e==r.iowaInsiderFeed()){var f=new Ad(g[u+3]);i.push(f),u++}if(0!=d&&0==d%3&&3>u&&e==r.magazineFeed()){var f=new Ad(g[u+6]);i.push(f),u++}if(null!=w.imageheight&&w.imageheight>150&&300>w.imageheight&&0==a&&e!=r.eventsFeed()){var f=new FeatureRow(w);a=!0,f.addEventListener("swipe",function(){l.fireEvent("swipeToggle")}),i.push(f)}else if(e==r.eventsFeed()){if(0==d||m!=w.pubDate&&0!=d){var p=new HeaderRow(w);if(0!=s&&0==s%3&&3>u){var f=new Ad(g[u]);i.push(f),u++,3==u&&(u=0)}i.push(p),s++}var f=new SingleRow(w);i.push(f)}else{var f=new Row(w);n>=1?(o.push(f),i.push(new PostGroup(o)),o=[],n=0,a=!1):(o.push(f),n++)}d++,m=w.pubDate}c.setData(i)}}function n(){d.loadRssFeed({success:function(e){a(e)}})}var r=new Feed,d=new RSS(e),l=Ti.UI.createView({backgroundColor:"#fff"}),c=new PostTable;if(c.addEventListener("scroll",function(e){var t=e.contentOffset.y;-65>t&&!c.pulling&&!c.eloading?(c.pulling=!0,c.updateLabelText("Release to refresh...")):t>-65&&0>t&&c.pulling&&!c.reloading&&(c.pulling=!1,c.updateLabelText("Pull down to refresh..."))}),c.addEventListener("dragEnd",function(){c.pulling&&!c.reloading&&(c.reloading=!0,c.pulling=!1,c.showActInd(),c.setContentInsets({top:60},{animated:!0}),c.scrollToTop(-60,!0),t())}),n(),c.bottom=70,l.add(c),e==r.iowaInsiderFeed())var s=new StaticAd(13,350);else if(e==r.magazineFeed())var s=new StaticAd(12,350);else var s=new StaticAd(10,350);return l.add(s),l}var Post=require("ui/common/Post"),FeatureRow=require("ui/common/FeatureRow"),Row=require("ui/common/Row"),SingleRow=require("ui/common/SingleRow"),HeaderRow=require("ui/common/HeaderRow"),IIBIntroRow=require("ui/common/IIBIntroRow"),IAMIntroRow=require("ui/common/IAMIntroRow"),PostGroup=require("ui/common/PostGroup"),PostTable=require("ui/common/PostTable"),Ad=require("ui/common/Ad"),GetFeed=require("ui/common/GetFeed"),FormatDate=require("ui/common/FormatDate"),RSS=require("services/rss"),WebView=require("ui/common/WebView"),StaticAd=require("ui/common/StaticAd");Feed=require("ui/common/Feed"),module.exports=MasterView;