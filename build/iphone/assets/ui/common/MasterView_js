function MasterView(e,t,i){function o(){l(),n()}function n(){u.setContentInsets({top:60},{animated:!0}),u.reloading=!1,u.updateLabelText("Refreshing..."),setTimeout(a,700)}function a(){u.setContentInsets({top:0},{animated:!0}),u.updateDateText("Last Updated: "+(new FormatDate).getDate()),u.hideActInd(),u.updateLabelText("Pull down to refresh...")}function r(o){if("[object Array]"===Object.prototype.toString.apply(o)){for(var n=[],a=[],r=!1,l=0,s=0,g=0,m=0,w=new GetFeed(d.adFeed()),h="",p=0;p<o.length;p++){var f=new Post(o[p]);if(0==p&&e==d.iowaInsiderFeed()){var b=new IIBIntroRow;n.push(b)}if(0==p&&e==d.magazineFeed()){var b=new IAMIntroRow;n.push(b)}if(0!=s&&s%3==0&&3>m&&e==d.iowaInsiderFeed()){var b=new Ad(w[m+3],i,t);n.push(b),m++}if(0!=s&&s%3==0&&3>m&&e==d.magazineFeed()){var b=new Ad(w[m+6],i,t);n.push(b),m++}if(null!=f.imageheight&&f.imageheight>150&&f.imageheight<300&&0==r&&e!=d.eventsFeed()){var b=new FeatureRow(f,i,t);r=!0,b.addEventListener("swipe",function(){c.fireEvent("swipeToggle")}),n.push(b)}else if(e==d.eventsFeed()){if(0==s||h!=f.pubDate&&0!=s){var T=new HeaderRow(f,i,t);if(0!=g&&g%3==0&&3>m){var b=new Ad(w[m],i,t);n.push(b),m++,3==m&&(m=0)}n.push(T),g++}var b=new SingleRow(f,i,t);n.push(b)}else{var b=new Row(f,i,t);l>=1?(a.push(b),n.push(new PostGroup(a)),a=[],l=0,r=!1):(a.push(b),l++)}s++,h=f.pubDate}u.setData(n)}}function l(){s.loadRssFeed({success:function(e){r(e)}})}var d=new Feed,s=new RSS(e),c=Ti.UI.createView({backgroundColor:"#fff"}),u=new PostTable;if(u.addEventListener("scroll",function(e){var t=e.contentOffset.y;-65>t&&!u.pulling&&!u.eloading?(u.pulling=!0,u.updateLabelText("Release to refresh...")):t>-65&&0>t&&u.pulling&&!u.reloading&&(u.pulling=!1,u.updateLabelText("Pull down to refresh..."))}),u.addEventListener("dragEnd",function(){u.pulling&&!u.reloading&&(u.reloading=!0,u.pulling=!1,u.showActInd(),u.setContentInsets({top:60},{animated:!0}),u.scrollToTop(-60,!0),o(),i.trackEvent({category:"Refreshing  Window",action:"refresh",label:"Refreshing "+t+"'s Window",value:1}))}),l(),u.bottom=70,c.add(u),e==d.iowaInsiderFeed())var g=new StaticAd(13,350,i,t);else if(e==d.magazineFeed())var g=new StaticAd(12,350,i,t);else var g=new StaticAd(10,350,i,t);return c.add(g),c}var Post=require("ui/common/Post"),FeatureRow=require("ui/common/FeatureRow"),Row=require("ui/common/Row"),SingleRow=require("ui/common/SingleRow"),HeaderRow=require("ui/common/HeaderRow"),IIBIntroRow=require("ui/common/IIBIntroRow"),IAMIntroRow=require("ui/common/IAMIntroRow"),PostGroup=require("ui/common/PostGroup"),PostTable=require("ui/common/PostTable"),Ad=require("ui/common/Ad"),GetFeed=require("ui/common/GetFeed"),FormatDate=require("ui/common/FormatDate"),RSS=require("services/rss"),WebView=require("ui/common/WebView"),StaticAd=require("ui/common/StaticAd");Feed=require("ui/common/Feed"),module.exports=MasterView;