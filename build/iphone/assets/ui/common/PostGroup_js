function PostGroup(e){for(var t=Ti.UI.createTableView({separatorColor:"d5d5d5",backgroundColor:"ffffff",height:"auto",width:300,left:10,top:10,bottom:0,padding:0,borderRadius:5,borderColor:"#d5d5d5",borderWidth:1,scrollable:!1}),i=0,o=0;e.length>o;o++)i+=e[o].height;t.height=i,t.setData(e);var a=Ti.UI.createTableViewRow({hasChild:!0,height:i+15,padding:0,top:0,bottom:0,layout:"vertical",selectionStyle:"none",backgroundColor:"e2e2e2"});return a.rightImage=null,a.backgroundSelectedImage=null,a.backgroundFocusImage=null,a.add(t),a}module.exports=PostGroup;