function StaticAd(e,t,i,o){var a=new Feed,n=new GetFeed(a.staticaAdFeed()),r=Ti.UI.createImageView({image:n[e].ad,width:320,height:70,bottom:0,left:0});return r.addEventListener("click",function(){new WebView(n[e].adUrl),i.trackEvent({category:"Ad",action:"click",label:"An Ad in the "+o+"'s Window - "+n[e].ad,value:1})}),r}var GetFeed=require("ui/common/GetFeed"),FormatDate=require("ui/common/FormatDate"),WebView=require("ui/common/WebView"),Feed=require("ui/common/Feed");module.exports=StaticAd;