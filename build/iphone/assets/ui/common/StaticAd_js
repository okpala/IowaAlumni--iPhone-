function StaticAd(e){var t=new Feed,i=new GetFeed(t.staticaAdFeed()),o=Ti.UI.createImageView({image:i[e].ad,width:320,height:70,bottom:0,left:0});return o.addEventListener("click",function(){new WebView(i[e].adUrl)}),o}var GetFeed=require("ui/common/GetFeed"),FormatDate=require("ui/common/FormatDate"),WebView=require("ui/common/WebView"),Feed=require("ui/common/Feed");module.exports=StaticAd;