function MapWindow(){for(var e=new Feed,t=Ti.UI.createView({backgroundColor:"#ffffff",navBarHidden:!0}),i=new GetFeed(e.iowaCityFeed()),a=[],o=0;i.length-1>=o;o++)a.push(Ti.Map.createAnnotation({latitude:i[o].latitude,longitude:i[o].longitude,title:i[o].company,subtitle:i[o].street,pincolor:Ti.Map.ANNOTATION_RED,animate:!0}));var n=Ti.Map.createView({mapType:Ti.Map.STANDARD_TYPE,region:{latitude:a[0].latitude,longitude:a[0].longitude,latitudeDelta:.01,longitudeDelta:.01},animate:!0,regionFit:!0,userLocation:!0,height:200,annotations:a,top:0});n.addEventListener("loading",function(){n.setLocation({latitude:a[0].latitude,longitude:a[0].longitude,latitudeDelta:.01,longitudeDelta:.01})}),n.addEventListener("postlayout",function(){n.setLocation({latitude:a[0].latitude,longitude:a[0].longitude,latitudeDelta:.01,longitudeDelta:.01})});var r=Ti.UI.createView({backgroundColor:"#e2e2e2",height:70,top:200}),d=Ti.UI.createLabel({text:"UI Alumni Association members have".concat("\nan array of available to them. Use your member benefit card at any of these locations."),textAlign:"left",left:10,width:300,top:10,font:{fontFamily:"HelveticaNeue-Light",fontSize:14,fontWeight:"bold"}});r.add(d);var l=Ti.UI.createLabel({text:"benefits",textAlign:"left",left:236.5,top:10,color:"blue",font:{fontFamily:"HelveticaNeue-Light",fontSize:14,fontWeight:"bold"}});l.addEventListener("click",function(){new WebView("http://iowalum.com/membership/benefits.cfm")}),r.add(l);for(var c=Ti.UI.createTableView({height:"auto",top:270}),s=[],o=0;i.length-1>=o;o++){if(0==o%2)var u=Ti.UI.createTableViewRow({company:i[o].company,latitude:i[o].latitude,longitude:i[o].longitude,height:"auto",bottom:10});else var u=Ti.UI.createTableViewRow({company:i[o].company,latitude:i[o].latitude,longitude:i[o].longitude,height:"auto",backgroundColor:"#cccccc",bottom:10});var g=Ti.UI.createLabel({text:i[o].company,textAlign:"left",height:20,top:10,left:10,font:{fontFamily:"Helvetica-Bold",fontSize:16,fontWeight:"normal"}}),m=Ti.UI.createLabel({text:i[o].discount,textAlign:"left",left:10,top:31,font:{fontFamily:"HelveticaNeue-Light",fontSize:12,fontWeight:"bold"}});u.add(g),u.add(m),s.push(u)}c.setData(s),t.add(n),t.add(r),t.add(c),c.addEventListener("click",function(e){n=Ti.Map.createView({mapType:Ti.Map.STANDARD_TYPE,region:{latitude:e.row.latitude,longitude:e.row.longitude,latitudeDelta:.01,longitudeDelta:.01},animate:!0,regionFit:!0,userLocation:!0,height:200,annotations:a,top:0}),t.add(n),n.selectAnnotation(a[e.index])});var h=new NavigateWindow("Iowa City Benefits",t);return h}var GetFeed=require("ui/common/GetFeed"),ApplicationWindow=require("ui/common/ApplicationWindow"),NavigateWindow=require("ui/common/NavigateWindow"),WebView=require("ui/common/WebView"),Feed=require("ui/common/Feed");module.exports=MapWindow;