function StatesWindow(e,t){t.trackScreen(e);var i=new Feed,o=Ti.UI.createView(),a=Ti.UI.createLabel({text:"Want to connect with fellow UI grads, need a place to watch the next game with fellow Hawkeye fans? IOWA clubs have you coveredâ€”find a location near you!",textAlign:"left",left:10,width:300,top:10,font:{fontFamily:"HelveticaNeue-Light",fontSize:14,fontWeight:"bold"}});o.add(a);var n=Ti.UI.createTableView({height:"auto",left:0,width:Ti.Platform.displayCaps.platformWidth,bottom:70,top:145}),r=Ti.UI.createImageView({image:"clubsPeople.png",left:10,width:Ti.Platform.displayCaps.platformWidth-20,top:85});o.add(r);for(var l=new GetFeed(i.clubsFeed()),d=new GetFeed(i.gameWatchFeed()),c=[],s=0,u=0;u<=d.length-1;u++)if(0==u||d[u-1].state!=d[u].state&&0!=u){if(s%2==0)var g=Ti.UI.createTableViewRow({text:d[u].state,height:50});else var g=Ti.UI.createTableViewRow({text:d[u].state,backgroundColor:"#cccccc",height:50});var h=Ti.UI.createLabel({text:d[u].state,textAlign:"center",font:{fontFamily:"Helvetica-Bold",fontSize:16,fontWeight:"normal"}});g.add(h),c.push(g),s++}n.setData(c),o.add(n),n.addEventListener("click",function(i){var o=getStateList(d,l,i.row.text),a=new GameWatchWindow(o[0],o[1],t,0),n=new ClubsandWatchesScrollWindow(o[0],o[1],t,a);n.open(),t.trackEvent({category:e,action:"click",label:i.row.text,value:1})});var m=new StaticAd(11,395,t,e);o.add(m);var w=new ApplicationWindow(e,o);return w}function getStateList(e,t,i){for(var o=[],a=[],n=[],r=0;r<=e.length-1;r++)e[r].state==i&&a.push(e[r]);o.push(a);for(var r=0;r<=t.length-1;r++)t[r].state.toUpperCase()==i&&n.push(t[r]);return o.push(n),o}var GetFeed=require("ui/common/GetFeed"),ClubsandWatchesScrollWindow=require("ui/common/ClubsandWatchesScrollWindow"),WebView=require("ui/common/WebView"),ApplicationWindow=require("ui/common/ApplicationWindow"),StaticAd=require("ui/common/StaticAd"),Feed=require("ui/common/Feed"),GameWatchWindow=require("ui/common/GameWatchWindow");module.exports=StatesWindow;