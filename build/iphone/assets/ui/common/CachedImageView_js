function CachedImageView(e,n,i){var t=n.split("/");t=t[t.length-1];var a=Ti.Filesystem.getFile(Ti.Filesystem.applicationDataDirectory,e,t);if(a.exists())i.image=a.nativePath,a=null;else{var r=Ti.Filesystem.getFile(Ti.Filesystem.applicationDataDirectory,e);r.exists()||r.createDirectory();var o=Ti.Network.createHTTPClient();o.onload=function(){200==o.status&&(a.write(o.responseData),i.image=a.nativePath,a=null)},o.open("GET",n),o.send()}}module.exports=CachedImageView;