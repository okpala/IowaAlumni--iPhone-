function CachedImageView(e,n,i){var t=n.split("/");t=t[t.length-1];var o=Ti.Filesystem.getFile(Ti.Filesystem.applicationDataDirectory,e,t);if(o.exists())i.image=o.nativePath,o=null;else{var a=Ti.Filesystem.getFile(Ti.Filesystem.applicationDataDirectory,e);a.exists()||a.createDirectory();var r=Ti.Network.createHTTPClient();r.onload=function(){200==r.status&&(o.write(r.responseData),i.image=o.nativePath,o=null)},r.open("GET",n),r.send()}}module.exports=CachedImageView;